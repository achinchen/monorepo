{
  "name": "remix-app-template",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && cross-env NODE_ENV=production remix build",
    "build:css": "unocss './app/**/*.{ts,tsx}' -o ./app/styles/uno.css",
    "dev": "concurrently 'npm run dev:css' 'npm run dev:remix' ",
    "dev:remix": "cross-env NODE_ENV=development remix dev",
    "dev:css": "unocss './app/**/*.{ts,tsx}' -o ./app/styles/uno.css --watch",
    "postinstall": "remix setup node"
  },
  "dependencies": {
    "@lottiefiles/lottie-player": "^1.5.6",
    "@remix-run/react": "^1.2.3",
    "@remix-run/vercel": "^1.2.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.2.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.2.3",
    "@remix-run/eslint-config": "^1.2.3",
    "@remix-run/serve": "^1.2.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@unocss/preset-attributify": "^0.30.12",
    "@unocss/preset-web-fonts": "^0.30.12",
    "@unocss/reset": "^0.30.12",
    "@unocss/transformer-directives": "^0.30.12",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5",
    "unocss": "^0.28.1"
  },
  "engines": {
    "node": ">=14"
  }
}
